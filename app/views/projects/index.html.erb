<p id="notice"><%= notice %></p>

<h1>Listing Projects</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th colspan="3">Project Name</th>
      <th colspan="3">Web URL</th>
      <th colspan="3">Readme URL</th>
      <th colspan="3">Student</th>
    </tr>
  </thead>

  <tbody>
    <% @projects.each do |project| %>
      <tr>
        <td colspan="3"><%= link_to project.name, project %></td>
        <td colspan="3"><%= project.web_url %></td>
        <td colspan="3"><%= project.read_me_url %></td>
        <td colspan="3"><%= project.student.name %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

