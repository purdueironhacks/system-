<p id="notice"><%= notice %></p>

<h1>Listing Projects</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"> Name </th>
      <th colspan="3"> Web URL </th>
      <th colspan="3">Student</th>
    </tr>
  </thead>

  <tbody>
    <% @projects.each do |project| %>
      <tr>
        <td colspan="3"><%= project.name %></td>
        <td colspan="3"><%= project.web_url %></td>
        <td colspan="3"><%= project.student.name %></td>
        <td colspan="3"><%= link_to 'Show', project %></td>
        <td colspan="3"><%= link_to 'Edit', edit_project_path(project) %></td>
        <td colspan="3"><%= link_to 'Destroy', project, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Project', new_project_path %> |
<%= link_to 'Students', students_path %> |
<%= link_to 'Graders', graders_path %>
